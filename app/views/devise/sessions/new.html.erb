<div class="col-md-4"></div>
    
    <div class="col-md-4">
      <section class="login-form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => {:id => "new-form"}) do |f| %>
<%= image_tag("/assets/logo2.png") %>
    <%= f.email_field :email, autofocus: true, :placeholder => "E-Mail", :class => "form-control input-lg" %>
    <%= f.password_field :password, autocomplete: "off", :placeholder => "Password", :class => "form-control input-lg" %>

  <button type="submit" name="go" class="btn btn-lg btn-primary btn-block">Sign in</button>
  <div>
  <%= link_to "Create account", new_user_registration_path %> (Or) <%= link_to "Reset Password", new_user_password_path %>
          </div>
<% end %>
</section>
</div>
